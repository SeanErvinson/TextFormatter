<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TextFormatterUI.Styles">
    

    <Color x:Key="Teal">#18BC9C</Color>
    <SolidColorBrush x:Key="TealBrush" Color="{StaticResource Teal}"/>

    <Color x:Key="Blue">#8AC0FF</Color>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource Blue}"/>

    <Color x:Key="Grey">#ECECEC</Color>
    <SolidColorBrush x:Key="GreyBrush" Color="{StaticResource Grey}"/>

    <Color x:Key="DarkBlue">#2C3E50</Color>
    <SolidColorBrush x:Key="DarkBlueBrush" Color="{StaticResource DarkBlue}"/>

    <Color x:Key="DarkGrey">#60606b</Color>
    <SolidColorBrush x:Key="DarkGreyBrush" Color="{StaticResource DarkGrey}"/>

    <Color x:Key="White">#FFFFFF</Color>
    <SolidColorBrush x:Key="TextColorBrush" Color="{StaticResource White}"/>

    <Color x:Key="AlternativeTextColor">#FFFFFF</Color>
    
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource Teal}"/>

    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>

    <SolidColorBrush x:Key="StatusBar" Color="{StaticResource DarkBlue}"/>

</ResourceDictionary>